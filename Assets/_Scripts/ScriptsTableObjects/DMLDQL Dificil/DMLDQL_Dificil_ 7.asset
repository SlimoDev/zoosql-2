  text_pregunta: "\xBFCu\xE1l instrucci\xF3n elimina solo los registros duplicados
    en la tabla clientes bas\xE1ndose en la columna email?"
  spr_pregunta: {fileID: 0}
  text_alternativas:
  - text_alternativa: DELETE FROM clientes WHERE email IS DUPLICATE;
    Is_correct: 0
  - text_alternativa: DELETE FROM clientes WHERE email IN (SELECT email FROM clientes
      GROUP BY email HAVING COUNT(email) > 1);
