  text_pregunta: "\xBFCu\xE1l consulta selecciona los nombres de los empleados que
    tienen un salario mayor al salario promedio de su propio departamento?"
  spr_pregunta: {fileID: 0}
  text_alternativas:
  - text_alternativa: SELECT nombre FROM empleados WHERE salario > (SELECT AVG(salario)
      FROM empleados);
    Is_correct: 0
  - text_alternativa: SELECT nombre FROM empleados WHERE salario > (SELECT AVG(salario)
      FROM empleados GROUP BY departamento);
    Is_correct: 0
  - text_alternativa: SELECT nombre FROM empleados WHERE salario > (SELECT AVG(salario)
      FROM empleados e WHERE e.departamento = empleados.departamento);
    Is_correct: 1
  - text_alternativa: SELECT nombre FROM empleados WHERE salario > AVG(salario) BY
      departamento;
    Is_correct: 0
