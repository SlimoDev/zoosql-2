  text_pregunta: "\xBFCu\xE1l consulta devuelve el salario promedio de todos los
    empleados cuyo departamento es 'Ventas'?"
  spr_pregunta: {fileID: 0}
  text_alternativas:
  - text_alternativa: SELECT AVG(salario) FROM empleados WHERE departamento = 'Ventas';
    Is_correct: 1
  - text_alternativa: SELECT SUM(salario) / COUNT(salario) FROM empleados WHERE departamento
      = 'Ventas';
    Is_correct: 0
  - text_alternativa: SELECT salario / COUNT(salario) FROM empleados GROUP BY departamento
      = 'Ventas';
    Is_correct: 0
  - text_alternativa: SELECT AVERAGE(salario) FROM empleados WHERE departamento =
      'Ventas';
